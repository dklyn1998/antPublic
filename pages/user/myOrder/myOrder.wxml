<!--顶部tab选项栏-->
<view class='tabs'>
    <view class='tabs_title'>
    <view 
    wx:for="{{tabs}}"
    wx:key="id"
    class='title_item {{item.isActive?"active":""}}'
    bindtap="handletap"
    data-index="{{index}}"
    >
        {{item.name}}
    </view>
    </view>
</view>

